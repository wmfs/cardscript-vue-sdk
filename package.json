{
  "name": "@wmfs/cardscript-vue-sdk",
  "version": "0.0.0-semantically-released",
  "description": "An SDK for using Cardscript with Vue/Vuetify",
  "main": "./lib/index.js",
  "keywords": [
    "tymly",
    "cardscript"
  ],
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "author": "West Midlands Fire Service",
  "license": "MIT",
  "devDependencies": {
    "chai": "6.2.2",
    "codecov": "3.8.3",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "cz-conventional-changelog": "3.3.0",
    "mocha": "11.7.1",
    "nyc": "17.1.0",
    "semantic-release": "25.0.2",
    "standard": "17.1.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@semantic-release/git": "10.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/cardscript-vue-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/cardscript-vue-sdk/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
