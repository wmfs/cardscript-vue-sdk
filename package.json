{
  "name": "@wmfs/cardscript-vue-sdk",
  "version": "0.0.0-semantically-released",
  "description": "An SDK for using Cardscript with Vue/Vuetify",
  "main": "./lib/index.js",
  "keywords": [
    "tymly",
    "cardscript"
  ],
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "author": "West Midlands Fire Service",
  "license": "MIT",
  "devDependencies": {
    "chai": "4.3.2",
    "codecov": "3.8.1",
    "conventional-changelog-metahub": "4.0.1",
    "cz-conventional-changelog": "3.3.0",
    "mocha": "8.3.0",
    "nyc": "15.1.0",
    "semantic-release": "17.4.0",
    "standard": "16.0.3",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/cardscript-vue-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/cardscript-vue-sdk/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
