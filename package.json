{
  "name": "@wmfs/cardscript-vue-sdk",
  "version": "0.0.0-semantically-released",
  "description": "An SDK for using Cardscript with Vue/Vuetify",
  "main": "./lib/index.js",
  "keywords": [
    "tymly",
    "cardscript"
  ],
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "author": "West Midlands Fire Service",
  "license": "MIT",
  "devDependencies": {
    "chai": "4.2.0",
    "codecov": "3.1.0",
    "conventional-changelog-metahub": "2.0.2",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "6.1.4",
    "nyc": "13.1.0",
    "semantic-release": "15.13.21",
    "standard": "14.0.2",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/cardscript-vue-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/cardscript-vue-sdk/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
